{
  "name": "eslint-config-standard-ext",
  "type": "module",
  "version": "2.2.3",
  "description": "An extended standard eslint configuration",
  "author": "",
  "license": "MIT",
  "keywords": [],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./src/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup-node src/index.js --format cjs && tsup-node src/index.js --dts-only --format esm",
    "test": "node --test",
    "posttest": "npm run lint && tsc",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "npm test && npm run build"
  },
  "dependencies": {
    "@antfu/eslint-config": "^3.11.2",
    "local-pkg": "^0.5.1"
  },
  "devDependencies": {
    "@types/node": "^20.12.12",
    "eslint": "^9.9.1",
    "eslint-astro-mate": "^0.0.3",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  }
}
